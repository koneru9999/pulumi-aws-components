{
  "name": "pulumi-aws-components",
  "version": "0.0.0",
  "description": "Reusable Pulumi components ",
  "private": true,
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md,json,yaml}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "prepublish": "lerna run build",
    "publish": "lerna publish",
    "lint": "eslint . --ext .ts,.md,.json,.yaml"
  },
  "author": "Venkaiah Chowdary Koneru <koneru.chowdary@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "12.12.47",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "husky": "4.2.5",
    "lerna": "3.22.1",
    "lint-staged": "10.2.10",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "typescript": "3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koneru9999/pulumi-aws-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
